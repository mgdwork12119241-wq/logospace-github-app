{
  "name": "Logospace AI",
  "url": "https://github.com/logospace/logospace-github-app",
  "hook_attributes": {
    "url": "https://logospace-ai.example.com/webhook"
  },
  "redirect_url": "https://logospace-ai.example.com/setup",
  "description": "Revolutionary GitHub App that transforms your code into 3D spatial consciousness. Analyze, visualize, and understand your codebase like never before.",
  "public": true,
  "default_events": [
    "pull_request",
    "push",
    "issues",
    "issue_comment"
  ],
  "default_permissions": {
    "contents": "read",
    "pull_requests": "read",
    "issues": "read",
    "metadata": "read"
  },
  "display_target": "api"
}
